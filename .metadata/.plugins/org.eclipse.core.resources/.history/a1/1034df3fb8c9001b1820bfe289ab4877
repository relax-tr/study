package snippet;

public class Snippet {
	SELECT        K.KRMKLNKOD, K.KRMKLNADI, K.ESKKRMKLNADI, K.TCKMLNMR, P.PBIK, LTRIM(RTRIM(P.ADI)) AS ADI, LTRIM(RTRIM(P.DGRADI)) AS DGRADI, LTRIM(RTRIM(P.SOYADI)) AS SOYADI, P.DGMTRH, P.DGMYER, P.DGMYERKOD, 
	                         K.BRLKOD, K.TMKKYTNMR, K.RTBKOD, K.GOREV, K.DAIRE, K.SUBE, K.TLFNMR, K.YTKBRLKOD, K.YTKACK, K.YTKSVY, K.KLNTIP, K.AKTKOD, K.KLLDRM, P.SNFRTBACK, P.SCLACK, P.GRVACK
	FROM            AS400.dbo.KRMKLNTML AS K INNER JOIN
	                         AS400.dbo.PRSKMLTML AS P ON K.TCKMLNMR = P.TCKMLNMR
}

